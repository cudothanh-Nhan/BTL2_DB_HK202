<Window x:Class="E_BookStore.GUI.Welcome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Welcome" Height="250" Width="400">
    <Grid>
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="10,10,0,0" x:Name="WelcomeHeading" Text="Welcome:" VerticalAlignment="Top" FontSize="17" FontStretch="ExtraCondensed"/>
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="90,10,0,0" x:Name="TextBlockCusID"  VerticalAlignment="Top" FontSize="15" FontStretch="ExtraCondensed" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="90,30,0,0" x:Name="TextBlockUsername"  VerticalAlignment="Top" FontSize="15" FontStretch="ExtraCondensed" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="90,50,0,0" x:Name="TextBlockRole"  VerticalAlignment="Top" FontSize="15" FontStretch="ExtraCondensed" />
    </Grid>
</Window>
